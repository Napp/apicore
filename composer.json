    "illuminate/contracts": "~5.4.0|~5.5.0",
    "illuminate/database": "~5.4.0|~5.5.0",
    "illuminate/http": "~5.4.0|~5.5.0",
    "illuminate/routing": "~5.4.0|~5.5.0",
    "illuminate/support": "~5.4.0|~5.5.0",
    "illuminate/validation": "~5.4.0|~5.5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.7",
    "mockery/mockery": "^0.9.5",
    "phpunit/phpunit" : "^6.1",
    "phpunit/phpcov": "^4.0",
    "squizlabs/php_codesniffer": "^3.1",
    "orchestra/testbench": "~3.0"
