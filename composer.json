{
  "name": "napp/apicore",
  "description": "API core for projects",
  "keywords": [
    "napp",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mads MÃ¸ller",
      "email": "mm@napp.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napp\\Core\\Api\\": "src/"
