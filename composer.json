{
  "name": "napp/apicore",
  "description": "API core for projects",
  "keywords": [
    "napp",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mads MÃ¸ller",
      "email": "mm@napp.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napp\\Core\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Napp\\Core\\Api\\Tests\\": "tests/"
    }
  },
  "require": {
    "php" : ">=7.1.3",
    "ext-json": "*",
    "illuminate/container": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/contracts": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/http": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/routing": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/validation": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/pagination": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit" : "^8.0|^9.0",
    "phpunit/phpcov": "^6.0|^7.0",
    "squizlabs/php_codesniffer": "^3.5",
    "orchestra/testbench": "^5.0",
    "orchestra/database": "^5.0",
    "fzaninotto/faker": "^1.9"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Napp\\Core\\Api\\Auth\\AuthServiceProvider",
        "Napp\\Core\\Api\\Requests\\Provider\\RequestServiceProvider",
        "Napp\\Core\\Api\\Router\\Provider\\RouterServiceProvider"
      ],
      "aliases": {}
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
